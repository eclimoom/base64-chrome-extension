{
  "name": "Base64 to Image",
  "version": "0.1.3",
  "description": "A base64 encoder/decoder extensions for the chrome",
  "manifest_version": 2,
  "default_locale": "en",
  "browser_action": {
    "default_popup": "index.html"
  },
  "icons": {
    "16": "assets/icons/16.png",
    "48": "assets/icons/48.png",
    "128": "assets/icons/128.png"
  },
  "permissions": [
    "clipboardRead"
  ]
}
